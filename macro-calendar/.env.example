# Public Supabase config (safe for browser)
NEXT_PUBLIC_SUPABASE_URL=https://your-project.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=your-anon-key

# Server-only secrets
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
UNSUBSCRIBE_TOKEN_SECRET=replace-with-openssl-rand-hex-32
ADMIN_UPLOAD_SECRET=replace-with-openssl-rand-hex-32
CRON_SECRET=replace-with-openssl-rand-hex-32

# Optional: enable request logging
ENABLE_REQUEST_LOGGING=false

# Optional: Upstash rate limiting
UPSTASH_REDIS_REST_URL=
UPSTASH_REDIS_REST_TOKEN=

# Optional: Stripe billing
STRIPE_SECRET_KEY=
STRIPE_WEBHOOK_SECRET=
STRIPE_PRICE_PLUS_MONTHLY=
STRIPE_PRICE_PLUS_YEARLY=
STRIPE_PRICE_PRO_MONTHLY=
STRIPE_PRICE_PRO_YEARLY=
STRIPE_PRICE_ENTERPRISE_MONTHLY=
STRIPE_PRICE_ENTERPRISE_YEARLY=

# Optional: Data import
FRED_API_KEY=
